{
    "manifest_version": 2,
    "name": "TimeSheet",
    "description": "Register worked hours.",
    "version": "1.0.0",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/assets/icon-16.png",
        "48": "/assets/icon-48.png",
        "128": "/assets/icon-128.png"
    }
}